module.exports = function(api) {
  api.cache(true);
  let plugins = [];
  
  <% if (props.useNativewind) { %>
    plugins.push('nativewind/babel');
  <% } %>

  <% if (props.navigationPackage?.name === 'expo-router') { %>
    plugins.push('expo-router/babel');
  <% } %>

  return {
    presets: ['babel-preset-expo'],
    plugins: plugins,
  };
};
